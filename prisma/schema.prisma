  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int        @id @default(autoincrement())
  name      String
  email     String     @unique
  password  String
  emailVerified Boolean    @default(false)
  categories Category[] 
}

model Category {
  id       Int      @id @default(autoincrement())
  name     String
  users    User[]   
}

